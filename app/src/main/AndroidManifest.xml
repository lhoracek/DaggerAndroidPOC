<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.generalbytes.myapplication">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".DependencyInjectionApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.SecondActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.FirstActivity" />
        <activity android:name=".activity.ThirdActivity" />

        <service android:name=".service.StickyService" />
        <service android:name=".mocks.service.EmulatorStickyService" />
        <service
            android:name=".service.ResultReceiverProcessService"
            android:process=":MyCustomProcess" />
        <service
            android:name=".service.MessengerProcessService"
            android:process=":OtherCustomProcess" />
    </application>

</manifest>